{
  "name": "yugioh-guessing-game",
  "version": "1.0.0",
  "main": "hub.js",
  "description": "CAPS - The Code Academy Parcel Service",
  "license": "MIT",
  "keywords": [],
  "author": "Reed",
  "scripts": {
    "test": "jest --verbose --coverage --detectOpenHandles --collectCoverageFrom='**/*.js' --coverageReporters='json' --coverageReporters='text' --coverageReporters='lcov' --coverageReporters='clover' --coverageReporters='html'",
    "start": "node hub.js",
    "build": "npm run build"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "chai": "^4.3.7",
    "chance": "^1.1.11",
    "colors": "^1.4.0",
    "dotenv": "^16.1.3",
    "express": "^4.18.2",
    "prompt-sync": "^4.2.0",
    "socket.io-client": "^4.6.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.22.1",
    "@babel/preset-env": "^7.22.4",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "socket.io": "^4.6.2",
    "socket.io-mock": "^1.3.2"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "react"
    ],
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error",
      "react/jsx-uses-react": "error",
      "react/jsx-uses-vars": "error",
      "no-console": "off"
    }
  },
  "directories": {
    "lib": "lib"
  }
}
